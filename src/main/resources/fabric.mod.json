{
  "schemaVersion": 1,
  "id": "vspoof",
  "version": "${version}",

  "name": "Vanilla Spoof",
  "description": "A mod that allows you to spoof the minecraft client as vanilla, instead of fabric.",
  "authors": [
    "8bitFra"
  ],
  "contact": {
    "homepage": "https://github.com/8bitFra/Vanilla-Spoof",
    "sources": "https://github.com/8bitFra/Vanilla-Spoof"
  },

  "license": "MIT License",
  "icon": "assets/vspoof/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.vanilla.spoof.Main"
    ]
  },
  "mixins": [
    "vspoof.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": ">=0.59.0",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
